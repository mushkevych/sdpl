# snippet: schema projection with and without EMIT

A = LOAD TABLE 'table_name' FROM 'tests/repo_a.yaml'
        WITH SCHEMA 'tests/schema_a.yaml' VERSION 1 ;
B = LOAD TABLE 'file_blob' FROM 'tests/repo_a.yaml'
        WITH SCHEMA 'tests/schema_b.yaml' VERSION 1;

D = SCHEMA PROJECTION (A.*, B.b);
E = SCHEMA PROJECTION (D.*, -B.*) EMIT;
Z = SCHEMA PROJECTION (D.a, D.aa, D.aaa) EMIT;

STORE E INTO TABLE 'table_name' FROM 'tests/repo_a.yaml';

